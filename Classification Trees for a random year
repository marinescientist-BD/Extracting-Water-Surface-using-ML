// Merge all samples
var trainingSamples = water.merge(nonWater);
Map.addLayer(water, {color: 'blue'}, 'Water Points');
Map.addLayer(nonWater, {color: 'green'}, 'Non-Water Points');

// Add random column for splitting
var withRandom = trainingSamples.randomColumn('random');

// Split into training (<0.7) and validation (>=0.7)
var trainingSet = withRandom.filter(ee.Filter.lt('random', 0.7));
var validationSet = withRandom.filter(ee.Filter.gte('random', 0.7));

print('Training samples:', trainingSet.size());
print('Validation samples:', validationSet.size());

var training = landsat7.sampleRegions({
  collection: trainingSet,
  properties: ['class'],
  scale: 30
});

// ========== TREE ANALYSIS SECTION ==========
// Define the number of trees to test
var numTreesList = [1, 5, 10, 15, 20, 25, 30];

// Function to test different numbers of trees
var testTrees = function(numTrees) {
  var classifier = ee.Classifier.smileRandomForest(numTrees).train({
    features: training,
    classProperty: 'class',
    inputProperties: landsat7.bandNames()
  });
  
  var classified = landsat7.classify(classifier);
  
  // Sample the classified image at validation points
  var validated = classified.sampleRegions({
    collection: validationSet,
    properties: ['class'],
    scale: 30
  });
  
  // Get confusion matrix
  var testAccuracy = validated.errorMatrix('class', 'classification');
  
  // Extract producer's accuracy (rows are reference, columns are predicted)
  var producerAccuracy = testAccuracy.producersAccuracy();
  var consumerAccuracy = testAccuracy.consumersAccuracy();
  
  return ee.Feature(null, {
    'numTrees': numTrees,
    'overallAccuracy': testAccuracy.accuracy(),
    'producerAccuracyNonWater': ee.List(producerAccuracy).get(0), // Class 0 (Non-Water)
    'producerAccuracyWater': ee.List(producerAccuracy).get(1),    // Class 1 (Water)
    'userAccuracyNonWater': ee.List(consumerAccuracy).get(0),     // Class 0 (Non-Water)
    'userAccuracyWater': ee.List(consumerAccuracy).get(1),        // Class 1 (Water)
    'kappaCoefficient': testAccuracy.kappa()
  });
};

// Map over the list of tree numbers
var results = ee.FeatureCollection(numTreesList.map(function(numTrees) {
  return testTrees(numTrees);
}));

// Print results
print('Accuracy Results for Different Numbers of Trees:', results);

// Create comprehensive accuracy chart
var chart = ui.Chart.feature.byFeature(results, 'numTrees', [
    'overallAccuracy', 
    'producerAccuracyWater', 
    'userAccuracyWater', 
    'kappaCoefficient'
  ])
  .setChartType('LineChart')
  .setOptions({
    title: 'Accuracy vs Classification Trees',
    hAxis: {
      title: 'Number of Classification Trees',
      titleTextStyle: {fontSize: 14, bold: true},
      textStyle: {fontSize: 12}
    },
    vAxis: {
      title: 'Accuracy',
      titleTextStyle: {fontSize: 14, bold: true},
      textStyle: {fontSize: 12},
      minValue: 0.7,
      maxValue: 1.0,
      format: '0.##'
    },
    series: {
      0: {color: '#1f77b4', lineWidth: 3, pointSize: 6, lineDashStyle: [1, 0]}, // Overall Accuracy - solid blue
      1: {color: '#ff7f0e', lineWidth: 3, pointSize: 6, lineDashStyle: [5, 5]}, // Producer's Accuracy Water - dashed orange
      2: {color: '#2ca02c', lineWidth: 3, pointSize: 6, lineDashStyle: [10, 5]}, // User's Accuracy Water - dash-dot green
      3: {color: '#d62728', lineWidth: 3, pointSize: 6, lineDashStyle: [15, 5, 5, 5]} // Kappa - dash-dot-dot red
    },
    legend: {
      position: 'bottom',
      alignment: 'center',
      textStyle: {fontSize: 12, bold: true}
    },
    backgroundColor: '#f8f9fa',
    chartArea: {
      left: 80,
      top: 60,
      width: '75%',
      height: '70%'
    },
    titleTextStyle: {fontSize: 16, bold: true, color: '#333'},
    interpolateNulls: false
  });

print(chart);byFeature(results, 'numTrees', [
    'producerAccuracyNonWater', 
    'userAccuracyNonWater'
  ])
  .setChartType('LineChart')
  .setOptions({
    title: 'Non-Water Class Accuracy vs Number of Classification Trees',
    hAxis: {
      title: 'Number of Classification Trees',
      titleTextStyle: {fontSize: 14, bold: true}
    },
    vAxis: {
      title: 'Accuracy Value',
      titleTextStyle: {fontSize: 14, bold: true},
      minValue: 0.7,
      maxValue: 1.0,
      format: '0.##'
    },
    series: {
      0: {color: '#9467bd', lineWidth: 3, pointSize: 6}, // Producer's Accuracy Non-Water - purple
      1: {color: '#8c564b', lineWidth: 3, pointSize: 6}  // User's Accuracy Non-Water - brown
    },
    legend: {position: 'bottom', alignment: 'center', textStyle: {fontSize: 12}},
    backgroundColor: '#f8f9fa'
  });

print(chartNonWater);
