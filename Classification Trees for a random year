var landsat7 = ee.ImageCollection("LANDSAT/LE07/C02/T1_L2")
  .filterDate('2000-11-01', '2000-12-31')
  .filterBounds(table)
  .median()
  .clip(table);

Map.addLayer(landsat7, {}, 'Landsat 7 RGB SR');
Map.centerObject(table);

//ndwi = (Green - nir)/(Green + nir)
var ndwi = landsat7.normalizedDifference(['SR_B2', 'SR_B4']).clip(table);
print(ndwi);
Map.addLayer(ndwi, {}, 'NDWI');

// Create binary NDWI: water if NDWI > 0
var ndwi_binary = ndwi.expression('b(0) > 0 ? 1 : 0').clip(table);
Map.addLayer(ndwi_binary, {}, 'NDWI Binary');

// Calculate WRI = (Green + Red) / (NIR + SWIR)
var wri = landsat7.expression(
  '((G + R) / (N + S))', {
    G: landsat7.select('SR_B2'),
    R: landsat7.select('SR_B3'),
    N: landsat7.select('SR_B4'),
    S: landsat7.select('SR_B5')
  }).clip(table);

Map.addLayer(wri, {}, 'WRI');

